<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="joc.hbm">
    <class name="domain.Joc"
           table="Joc">
        <id name="id" column="id_joc" type="integer">
            <generator class="increment"/>
        </id>
        <property
                type="string"
                name="cuvant1"
                column="cuvant1"/>
        <property
                type="string"
                name="cuvant2"
                column="cuvant2"/>
        <property
                type="string"
                name="cuvant3"
                column="cuvant3"/>
        <property
                type="string"
                name="litera1"
                column="litera1"/>
        <property
                type="string"
                name="litera2"
                column="litera2"/>
        <property
                type="string"
                name="litera3"
                column="litera3"/>

    </class>
</hibernate-mapping>